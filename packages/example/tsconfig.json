{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "include": ["src", "uno.config.ts"],
  "compilerOptions": {
    "resolveJsonModule": true,
    "noUncheckedIndexedAccess": false,
    "exactOptionalPropertyTypes": false,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": []
  }
}
